        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Boss Special Attack
attacking = true;

if( phase == 1){ // Melee Phase
    image_speed = .5;
    sprite_index = spr_Boss_Attack_Whirlwind;
    instance_create(x,y,obj_Boss_Whirlwind);
    alarm[5] = room_speed/4;

} else if( phase == 2){ // Mage Phase
    sprite_index = spr_Boss_Casting;
    crouched = floor(random(2)) + 1 // 2 sided dice
        
    if( crouched == 1 )
        sprite_index = spr_Boss_Crouch_Casting;
        
    image_index = 0;
    image_speed = .1;
    alarm[4] = .4 * room_speed;
       
} else {  // phase 3 - Mage/Warrior
    d2 = floor(random(2)) + 1 // 2 sided dice
    if( d2 == 1){ // Magic
        sprite_index = spr_Boss_Casting;
        crouched = floor(random(2)) + 1 // 2 sided dice
        
        if( crouched == 1 )
            sprite_index = spr_Boss_Crouch_Casting;
        
        image_index = 0;
        image_speed = .1;
        alarm[4] = .4 * room_speed;    
    } else { // Whirlwind
        image_speed = .5;
        sprite_index = spr_Boss_Attack_Whirlwind;
        instance_create(x,y,obj_Boss_Whirlwind);
        alarm[5] = room_speed/4;
    }
}    
